{
  "$schema": "https://json.schemastore.org/chrome-manifest.json",
  "manifest_version": 3,
  "name": "Kiekeboe",
  "description": "Personal Homepage",
  "version": "%VERSION%",
  "key": "",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "resources": ["spotify-sdk.min.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "commands": {
    "toggle-command-center": {
      "suggested_key": {
        "default": "Ctrl+Shift+P",
        "mac": "Command+Shift+P"
      },
      "description": "Toggle Command Center"
    }
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "storage",
    "identity",
    "alarms",
    "notifications",
    "geolocation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "oauth2": {
    "client_id": "%CLIENT_ID%",
    "scopes": [
      "https://www.googleapis.com/auth/photoslibrary.readonly",
      "https://www.googleapis.com/auth/tasks",
      "profile",
      "email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  }
}
