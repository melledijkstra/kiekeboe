{
  "name": "@odysea/server",
  "version": "0.0.1",
  "description": "Server for the Odysea ecosystem",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "migrate": "tsx scripts/migrate --knexfile ./knexfile.ts migrate:latest"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "knex": "^3.1.0",
    "sqlite3": "^5.1.7",
    "tsx": "^4.19.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1"
  }
}
